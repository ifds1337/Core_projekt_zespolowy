@model IEnumerable<ProjZesp.Models.Rezerwacje>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Rez_Do)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Rez_ID_Klienta)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Rez_ID_Pokoju)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Rez_Od)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Rez_Do)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Rez_ID_Klienta)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Rez_ID_Pokoju)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Rez_Od)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
