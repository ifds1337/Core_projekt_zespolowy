@model IEnumerable<ProjZesp.Models.Klient>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Klient_Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Klient_Imie)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Klient_Nazwisko)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Klient_NrTelefonu)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Klient_Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Klient_Imie)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Klient_Nazwisko)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Klient_NrTelefonu)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
